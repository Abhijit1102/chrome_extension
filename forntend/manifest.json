{
  "manifest_version": 3,
  "name": "AI Chat Extension",
  "version": "1.0",
  "permissions": ["cookies", "tabs", "activeTab", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "icons/icon.png"
    }
  }
}
